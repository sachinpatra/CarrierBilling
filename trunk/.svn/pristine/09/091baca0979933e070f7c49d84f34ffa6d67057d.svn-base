#import <Foundation/Foundation.h>

@interface SettingModelMqtt : NSObject <NSCoding>
{
    NSString* _chatTopic;
    NSString* _chatHostName;
    NSInteger _chatPortSSL;
    NSString* _chatUser;
    NSString* _chatPassword;
    NSString* _mqttHostName;
    NSInteger _mqttPortSSL;
    NSString* _mqttPassword;
    NSString* _mqttUser;
    NSNumber* _deviceId;
}

@property(nonatomic,strong)NSString* chatTopic;
@property(nonatomic,strong)NSString* chatHostName;
@property(nonatomic)NSInteger chatPortSSL;
@property(nonatomic,strong)NSString* chatUser;
@property(nonatomic,strong)NSString* chatPassword;
@property(nonatomic,strong)NSString* mqttHostName;
@property(nonatomic)NSInteger mqttPortSSL;
@property(nonatomic,strong)NSString* mqttPassword;
@property(nonatomic,strong)NSString* mqttUser;
@property(nonatomic,strong)NSNumber* deviceId;

@end
